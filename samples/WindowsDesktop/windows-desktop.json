{
  "Items": [
    {
      "Title": "Audio\nSettings",
      "Items": [
        {
          "Title": {
            "PowerShell": "Import-Module AudioDeviceCmdlets -ErrorAction SilentlyContinue; $device = Get-AudioDevice -Recording; if ($device.Device.AudioEndpointVolume.Mute) { '{\"text\": \"MIC\nMUTED\", \"theme\": \"toggled\"}' } else { 'MIC\nACTIVE' }",
            "Refresh": "1s"
          },
          "Action": {
            "PowerShell": "Import-Module AudioDeviceCmdlets -ErrorAction SilentlyContinue; Set-AudioDevice -RecordingMuteToggle"
          }
        },
        {
          "Title": {
            "PowerShell": "Import-Module AudioDeviceCmdlets -ErrorAction SilentlyContinue; $device = Get-AudioDevice -Playback; if ($device.Device.AudioEndpointVolume.Mute) { '{\"text\": \"AUDIO\nMUTED\", \"theme\": \"toggled\"}' } else { 'AUDIO\nACTIVE' }",
            "Refresh": "1s"
          },
          "Action": {
            "PowerShell": "Import-Module AudioDeviceCmdlets -ErrorAction SilentlyContinue; Set-AudioDevice -PlaybackMuteToggle"
          }
        },
        {
          "Title": "Select\nMicrophone",
          "DynamicSubmenu": {
            "PowerShell": "Import-Module AudioDeviceCmdlets -ErrorAction Stop; Get-AudioDevice -List | Where-Object Type -eq 'Recording' | ForEach-Object { @{title=$_.Name; action=@{powershell=\"Import-Module AudioDeviceCmdlets; Set-AudioDevice -Index $($_.Index)\"}} } | ConvertTo-Json -AsArray"
          }
        },
        {
          "Title": "Select\nSpeaker",
          "DynamicSubmenu": {
            "PowerShell": "Import-Module AudioDeviceCmdlets -ErrorAction Stop; Get-AudioDevice -List | Where-Object Type -eq 'Playback' | ForEach-Object { @{title=$_.Name; action=@{powershell=\"Import-Module AudioDeviceCmdlets; Set-AudioDevice -Index $($_.Index)\"}} } | ConvertTo-Json -AsArray"
          }
        }
      ],
      "Theme": "utility"
    },
