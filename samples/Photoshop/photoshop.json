{
  "global": {
    "monitorIndex": 1,
    "profilename": "Photoshop",
    "refresh": "30s",
    "activewindow": "Photoshop.exe"
  },
  "items": [
    {
      "action": {
        "keypress": "^n"
      },
      "title": "NEW DOCUMENT"
    },
    {
      "action": {
        "keypress": "^o"
      },
      "title": "OPEN"
    },
    {
      "action": {
        "keypress": "^s"
      },
      "title": "SAVE"
    },
    {
      "action": {
        "keypress": "^+s"
      },
      "title": "SAVE AS"
    },
    {
      "action": {
        "keypress": "^+n"
      },
      "title": "NEW LAYER"
    },
    {
      "action": {
        "keypress": "^j"
      },
      "title": "DUPLICATE LAYER"
    },
    {
      "action": {
        "keypress": "^e"
      },
      "title": "MERGE DOWN"
    },
    {
      "action": {
        "keypress": "^+e"
      },
      "title": "MERGE VISIBLE"
    },
    {
      "action": {
        "keypress": "v"
      },
      "title": "MOVE TOOL"
    },
    {
      "action": {
        "keypress": "b"
      },
      "title": "BRUSH TOOL"
    },
    {
      "action": {
        "keypress": "e"
      },
      "title": "ERASER TOOL"
    },
    {
      "action": {
        "keypress": "l"
      },
      "title": "LASSO TOOL"
    },
    {
      "action": {
        "keypress": "^t"
      },
      "title": "FREE TRANSFORM"
    },
    {
      "action": {
        "keypress": "^z"
      },
      "title": "UNDO"
    },
    {
      "action": {
        "keypress": "^!z"
      },
      "title": "STEP BACKWARD"
    },
    {
      "action": {
        "keypress": "^+z"
      },
      "title": "REDO"
    },
    {
      "action": {
        "keypress": "^d"
      },
      "title": "DESELECT"
    },
    {
      "action": {
        "keypress": "^+i"
      },
      "title": "INVERSE SELECT"
    },
    {
      "action": {
        "keypress": "{TAB}"
      },
      "title": "TOGGLE PANELS"
    },
    {
      "title": {
        "builtin": "clock()",
        "refresh": "1s",
        "theme": "prominent"
      }
    }
  ],
  "themes": [
    {
      "name": "default",
      "foreground": "#FF6B35",
      "background": "#1A1A2E"
    },
    {
      "name": "prominent",
      "foreground": "#000000",
      "background": "#FF6B35"
    }
  ]
}
