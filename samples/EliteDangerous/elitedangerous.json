{
  "global": {
    "monitorIndex": 1,
    "profilename": "EliteDangerous",
    "refresh": "30s",
    "activewindow": "EliteDangerous64.exe"
  },
  "items": [
    {
      "action": {
        "keypress": "s"
      },
      "title": "SHIELD CELL"
    },
    {
      "action": {
        "keypress": "g"
      },
      "title": {
        "python": [
          "%ProgramFiles%/MacroButtons/samples/EliteDangerous/elite-status",
          "--gear"
        ],
        "refresh": "1s"
      }
    },
    {
      "action": {
        "keypress": "{home}"
      },
      "title": {
        "python": [
          "%ProgramFiles%/MacroButtons/samples/EliteDangerous/elite-status",
          "--scoop"
        ],
        "refresh": "1s"
      }
    },
    {
      "action": {
        "keypress": "c"
      },
      "title": "CHAFF"
    },
    {
      "title": {
        "builtin": "clock()",
        "refresh": "1s",
        "theme": "prominent"
      }
    },
    {
      "action": {
        "keypress": "h"
      },
      "title": "HEAT SINK"
    },
    {
      "action": {
        "keypress": "{F5}"
      },
      "title": "COMBAT/SENSOR TOGGLE"
    },
    {
      "title": ""
    },
    {
      "action": {
        "keypress": "{space}"
      },
      "title": "DISCOVERY SCANNER"
    },
    {
      "items": [
        {
          "title": "DOCKING"
        },
        {
          "title": "DEFENSIVE"
        },
        {
          "title": "ENGAGE AT WILL"
        },
        {
          "title": "ATTACK TARGET"
        }
      ],
      "title": "FIGHTER ORDERS"
    },
    {
      "action": {
        "keypress": "o"
      },
      "title": "COLONISATION"
    },
    {
      "items": [
        {
          "action": {
            "keypress": "!{F1}"
          },
          "title": "FREE CAMERA"
        },
        {
          "action": {
            "keypress": "^!{space}"
          },
          "title": "TOGGLE HUD"
        },
        {
          "action": {
            "keypress": "!{NUMPADADD}"
          },
          "title": "ZOOM IN"
        },
        {
          "action": {
            "keypress": "!{NUMPADSUB}"
          },
          "title": "ZOOM OUT"
        },
        {
          "action": {
            "keypress": "!r"
          },
          "title": "RESET CAMERA"
        },
        {
          "action": {
            "keypress": "{F10}"
          },
          "title": "HIGH RES SCREENSHOT"
        }
      ],
      "title": "CAMERA SUITE"
    },
    {
      "action": {
        "keypress": "]"
      },
      "title": "SYSTEM MAP"
    },
    {
      "action": {
        "keypress": "["
      },
      "title": "GALAXY MAP"
    },
    {
      "action": {
        "keypress": "p"
      },
      "title": "G.A.L.N.E.T"
    }
  ],
  "themes": [
    {
      "name": "default",
      "foreground": "green",
      "background": "black"
    },
    {
      "name": "toggled",
      "foreground": "black",
      "background": "orange"
    },
    {
      "name": "prominent",
      "foreground": "#000000",
      "background": "#288F28"
    }
  ]
}
