{
  "global": {
    "monitorindex": 0,
    "profilename": "default",
    "refresh": "30s",
    "sendkeys": {
      "delay": "10ms",
      "duration": "30ms"
    }
  },
  "items": [
    {
      "action": {
        "keypress": "^c"
      },
      "title": "Copy"
    },
    {
      "action": {
        "keypress": "^v"
      },
      "title": "Paste"
    },
    {
      "action": {
        "exe": "%SystemRoot%/system32/calc.exe"
      },
      "title": "Calculator"
    },
    {
      "action": {
        "builtin": "quit()"
      },
      "title": "Quit"
    },
    {
      "title": {
        "builtin": "clock()",
        "refresh": "1s",
        "theme": "prominent"
      }
    }
  ],
  "theme": [
    {
      "name": "default",
      "background": "black",
      "foreground": "green"
    },
    {
      "name": "toggled",
      "background": "orange",
      "foreground": "black"
    },
    {
      "name": "toggled",
      "background": "orange",
      "foreground": "black"
    },
    {
      "name": "prominent",
      "foreground": "#000000",
      "background": "#288F28"
    }
  ]
}
